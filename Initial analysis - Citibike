nrow(df1)
nrow(df2)
df<- rbind(df1,df2,df3,df4,df5,df6,df7,df8,df9,df10,df11,df12)
nrow(df)
m<-median(df$tripduration,na.rm=FALSE)
formattable(m,digits=10,format="f")
m

df.station<-subset(df, df$start.station.id==df$end.station.id)
nrow(df.station)
fraction<-nrow(df.station)/nrow(df)
x <- formattable(fraction,digits=10,format="f")
x

avg1<-mean(df1$tripduration)
avg2<-mean(df2$tripduration)
avg3<-mean(df3$tripduration)
avg4<-mean(df4$tripduration)
avg5<-mean(df5$tripduration)
avg6<-mean(df6$tripduration)
avg7<-mean(df7$tripduration)
avg8<-mean(df8$tripduration)
avg9<-mean(df9$tripduration)
avg10<-mean(df10$tripduration)
avg11<-mean(df11$tripduration)
avg12<-mean(df12$tripduration)

avg <- c(avg1,avg2,avg3,avg4,avg5,avg6,avg7,avg8,avg9,avg10,avg11,avg12)

min(avg)
max(avg)
final_avg<-max(avg)-min(avg)
final_avg <- formattable(final_avg,digits=10,format="f")
final_avg
